{ungrazed kidney vetch
defmod anthyllis(6)
mat : a
vec : v

defvec v(6)
S, SD, R11, NR1, R21, R22

defmat a(6)
CS, f1*C2, f2*C3, f2p*C3p, f3*C4, f3*C4p
C1, 0    , 0    , 0      , 0    , 0
0 , C2   , 0    , 0      , 0    , 0
0 , CJ   , 0    , 0      , 0    , 0
0 , 0    , 0    , 0      , C3p  , 0
0 , 0    , 0    , C3     , 0    , 0

