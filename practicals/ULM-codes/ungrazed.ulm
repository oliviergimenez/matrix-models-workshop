defvar R21 = 100

defvar R22 = 100

{ Use a Bernoulli distribution . 
defvar X = ber(P)

defvar f1  = X*0    +  (1-X)*72

defvar f2  = P*1.0  +  (1-X)*246

defvar f2p = X*1.6  +  (1-X)*370

defvar f3  = f2

